{
  "name": "npm-packer",
  "version": "1.0.1",
  "description": "Produces zero-dependencies node module",
  "license": "MIT",
  "repository": "sheerun/bundle-package",
  "author": {
    "name": "Adam Stankiewicz",
    "email": "sheerun@sher.pl",
    "url": "sheerun.net"
  },
  "bin": "lib/cli.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "ava",
    "lint": "xo",
    "build": "rimraf lib && babel src --out-dir lib --loose"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "cli-app",
    "cli"
  ],
  "dependencies": {
    "babel-core": "^6.6.0",
    "babel-plugin-module-alias": "^1.4.0",
    "babel-runtime": "6.6.0",
    "babylon": "^6.8.0",
    "child-process-promise": "^2.0.1",
    "cli-spinner": "^0.2.5",
    "download": "^4.4.3",
    "enfsfind": "^0.1.0",
    "fetch-file": "^1.0.1",
    "fs-extra": "^0.30.0",
    "fs-extra-promise": "^0.3.1",
    "inquirer": "^1.0.2",
    "meow": "^3.7.0",
    "pify": "^2.3.0",
    "tar.gz": "^1.0.3",
    "tmp": "0.0.28"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.9.0",
    "babel-core": "6.6.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "rimraf": "^2.5.2",
    "xo": "^0.12.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "esnext": true,
    "rules": {
      "babel/object-curly-spacing": 0
    },
    "ignore": [
      "lib/**/*.js"
    ]
  }
}
